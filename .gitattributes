# Large binary/data files - ALWAYS LFS
*.owl filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.seg filter=lfs diff=lfs merge=lfs -text
*.npy filter=lfs diff=lfs merge=lfs -text
*.parquet filter=lfs diff=lfs merge=lfs -text
*.gz filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text

# Large TSV files (ontologies)
data/ontologies/*.tsv filter=lfs diff=lfs merge=lfs -text
# Whoosh index segments
data/**/*.seg filter=lfs diff=lfs merge=lfs -text
# Large JSON data files (embeddings, ontology dumps, metadata)
data/**/embeddings.json filter=lfs diff=lfs merge=lfs -text
data/**/ontology_dump.json filter=lfs diff=lfs merge=lfs -text
data/**/faiss_metadata.json filter=lfs diff=lfs merge=lfs -text
# Benchmark result details (large)
data/benchmark_results/**/details.json filter=lfs diff=lfs merge=lfs -text
data/benchmark_results/**/results.jsonl filter=lfs diff=lfs merge=lfs -text
# Large cache files
data/**/cache*.json filter=lfs diff=lfs merge=lfs -text
# Log files (can grow large)
logs/*.log filter=lfs diff=lfs merge=lfs -text
# Analysis data
data/analysis/*.csv filter=lfs diff=lfs merge=lfs -text
data/chebi/analysis/*.json filter=lfs diff=lfs merge=lfs -text
requirements.txt !text !filter !merge !diff
data/benchmark_results/**/summary.json !text !filter !merge !diff
data/*/results.json !text !filter !merge !diff
data/benchmark_results/comparison.json !text !filter !merge !diff
data/ontologies/gene_info.tsv !text !filter !merge !diff
